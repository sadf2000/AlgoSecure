{
  "manifest_version": 3,
  "name": "AlgoSecure Beta | Защита для Алгоритмики",
  "version": "2.0",
  "permissions": [
    "declarativeNetRequest",
    "alarms" 
  ],
  "host_permissions": [
    "*://jsonalgosecure.netlify.app/*",
    "*://learn.algoritmika.org/*"
  ],
  "background": {
    "service_worker": "bg.js",
    "type": "module"  
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png" 
  },
  "icons": {
    "128": "icons/icon.png" 
  },
  "web_accessible_resources": [
    {
      "resources": ["blocked.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
